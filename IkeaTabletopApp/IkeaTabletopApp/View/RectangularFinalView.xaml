<Page
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IkeaTabletopApp.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ViewModel="using:IkeaTabletopApp.ViewModel"
    x:Class="IkeaTabletopApp.View.RectangularFinalView"
    mc:Ignorable="d">

    <Page.DataContext>
        <ViewModel:RectangularFinalVM/>
    </Page.DataContext>

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <Rectangle Fill="#FFF4F4F5" HorizontalAlignment="Left" Height="{Binding Width, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Margin="509,290,0,0" Stroke="Black" VerticalAlignment="Top" Width="{Binding Length, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
            
    
        <TextBlock x:Name="Pris" HorizontalAlignment="Left" Margin="591,151,0,0" TextWrapping="Wrap" Text="Pris" VerticalAlignment="Top" Height="110" Width="326" FontSize="72"/>
        <TextBlock x:Name="SamletPris" HorizontalAlignment="Left" Margin="970,151,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="110" Width="326" FontSize="72"/>

    </Grid>
</Page>
